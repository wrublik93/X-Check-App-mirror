{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "module": "ES2015",
    "target": "ES6",
    "baseUrl": "src",
    "outDir": "./build/",
    "rootDir": "./",
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ],
  }
}
